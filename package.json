{
  "name": "@daks.dev/.dev.daks",
  "version": "0.1.1",
  "description": "DAKS portfolio draft",
  "private": false,
  "type": "module",
  "scripts": {
    "info": "astro info",
    "start": "astro dev --port=3000",
    "dev": "astro dev --local-development --port=3000",
    "build": "astro build",
    "preview": "astro preview --port=3000",
    "check": "astro check && svelte-check",
    "check:watch": "astro check --watch",
    "check:watch:svelte": "svelte-check --tsconfig ./tsconfig.svelte.json --watch",
    "test:integration": "playwright test -c tests/",
    "test:unit": "vitest --ui",
    "test": "pnpm test:integration && pnpm test:unit",
    "run:prettier": "prettier --plugin=prettier-plugin-astro --plugin=prettier-plugin-svelte --plugin=prettier-plugin-tailwindcss",
    "lint": "pnpm run:prettier --check . && eslint .",
    "format": "pnpm run:prettier --write .",
    "run:update": "pnpm up"
  },
  "dependencies": {
    "@astrojs/check": "^0.2.1",
    "@astrojs/mdx": "^1.1.3",
    "@astrojs/node": "^6.0.3",
    "@astrojs/prefetch": "^0.4.1",
    "@astrojs/react": "^3.0.4",
    "@astrojs/rss": "^3.0.0",
    "@astrojs/sitemap": "^3.0.3",
    "@astrojs/svelte": "^4.0.3",
    "@astrojs/tailwind": "^5.0.2",
    "@astrojs/ts-plugin": "^1.3.1",
    "@nanostores/persistent": "^0.9.1",
    "@playwright/test": "^1.39.0",
    "@types/node": "^20.8.10",
    "@types/postcss-import": "^14.0.2",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.14",
    "@vitest/ui": "^0.34.6",
    "astro": "^3.4.3",
    "astro-compress": "^2.2.3",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "cssnano-preset-advanced": "^6.0.1",
    "nanostores": "^0.9.4",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "svelte": "^4.2.2",
    "svelte-check": "^3.5.2",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=16.20.2",
    "pnpm": "^8.0.0"
  },
  "browserslist": [
    "last 2 version",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/daks-dev/.dev.daks"
  },
  "bugs": "https://github.com/daks-dev/.dev.daks/issues",
  "author": {
    "name": "DVA",
    "email": "mail@daks.dev",
    "url": "https://daks.dev"
  },
  "homepage": "https://daks.dev/",
  "keywords": [
    "astro",
    "react",
    "svelte",
    "vite"
  ]
}
